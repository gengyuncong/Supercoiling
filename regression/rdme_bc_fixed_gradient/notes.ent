lm_setdm rdme_bc_fixed_gradient_global.lm numberReactions=2 numberSpecies=3 numberSiteTypes=1 "latticeSize=[10,10,10]" latticeSpacing=1e-6 particlesPerSite=8 "DiffusionMatrix=[8.15e-14]" "ReactionLocationMatrix=[0]";
lm_setp rdme_bc_fixed_gradient_global.lm maxTime=1e4 writeInterval=1e2 latticeWriteInterval=1e3
lm_setp rdme_bc_fixed_gradient_global.lm boundaryConditions=FIXED_GRADIENT boundarySite=0 boundarySpecies=2
python rdme_build_gradient.py rdme_bc_fixed_gradient_global.lm 10 10 10 x 1.66e-9 1.66e-9
~/Work/Lab/Code/LatticeMicrobes/build/lm -f rdme_bc_fixed_gradient_global.lm --resource-map=nodes -ca -ff log -sp |tee rdme_bc_fixed_gradient_global.log

