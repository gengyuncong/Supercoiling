keywords = {Computational Biology, Escherichia coli, Models: Biological, Monte Carlo Method, Computer Simulation, Transcription: Genetic, Signal Transduction, Gene Expression Profiling, Gene Regulatory Networks, Reproducibility of Results, Metabolic Networks and Pathways, Genome: Bacterial}, 
