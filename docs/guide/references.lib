language = {eng},
keywords = {Cytosol, Escherichia coli, Mass Spectrometry, Chromatography: Liquid, Escherichia coli Proteins, Proteomics}, 
date-added = {2011-02-18 11:33:36 -0600},
date-modified = {2011-02-18 11:33:36 -0600},
