language = {eng},
keywords = {Lac Operon, Gene Expression Regulation: Bacterial, Stochastic Processes, Models: Statistical, Escherichia coli, Computer Simulation, beta-Galactosidase, Gene Expression Regulation: Enzymologic, Evolution: Molecular, Models: Genetic}, 
date-added = {2010-06-04 10:15:20 -0500},
date-modified = {2010-06-04 10:15:20 -0500},
doi = {10.1371/journal.pcbi.0030111},
pii = {06-PLCB-RA-0458},
