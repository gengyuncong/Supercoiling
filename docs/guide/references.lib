language = {eng},
keywords = {DNA-Binding Proteins, Escherichia coli, Gene Expression Profiling, DNA, Binding Sites, Chromosomes: Bacterial, RNA: Messenger, SYSBIO, Escherichia coli Proteins, Genome: Bacterial, Hybridization: Genetic, DNA-Directed RNA Polymerases, DNA Footprinting}, 
